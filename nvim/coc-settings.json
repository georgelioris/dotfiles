{
  "npm.binPath": "~/.yarn/bin",
  "snippets.extends": {
    "svelte": ["javascript"],
    "javascriptreact": ["javascript"],
    "typescriptreact": ["typescript"],
    "typescript": ["javascript"],
    "typescript.jsx": ["typescriptreact"]
  },
  "stylelint.configOverrides": {
    "rules": {
      "unit-allowed-list": ["px", "em", "deg", "rem", "vw", "vh", "%"]
    }
  },
  "languageserver": {
    "intelephense": {
      "command": "intelephense",
      "args": ["--stdio"],
      "filetypes": ["php"],
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    }
  },
  "suggest.snippetIndicator": " â‡Œ",
  "snippets.userSnippetsDirectory": "~/.config/coc/textmate",
  "snippets.textmateSnippetsRoots": ["~/.config/coc/textmate"],
  "snippets.ultisnips.enable": false,
  "emmet.includeLanguages": { "svelte": "html", "blade": "html" },
  "python.analysis.diagnosticEnabled": false,
  "python.linting.enabled": false,
  "python.jediEnabled": false,
  "codeLens.enable": false,
  "javascript.implementationsCodeLens.enable": false,
  "javascript.referencesCodeLens.enable": false,
  "typescript.implementationsCodeLens.enable": false,
  "typescript.referencesCodeLens.enable": false,
  "diagnostic.displayByAle": true,
  "suggest.noselect": false,
  "javascript.suggestionActions.enabled": false,
  "javascript.validate.enable": false,
  "typescript.suggestionActions.enabled": false,
  "typescript.validate.enable": false,
  "suggest.floatEnable": true,
  "diagnostic.enable": false,
  "signature.enable": true
}
